


  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build"
      }
    }
  ],
  "env": {
    "VARIABLE_NAME": "value"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.html"
    }

  ]
}
}
